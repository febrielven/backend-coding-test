{
  "name": "backend-coding-test",
  "version": "1.0.0",
  "description": "A coding assessment for EmpatKali backend interviews",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint ./src",
    "test": "mocha tests",
    "coverage": "nyc --reporter=text npm run test",
    "start": "node index.js"
  },
  "nyc": {
    "include": "src",
    "exclude": "specs"
  },
  "keywords": [
    "Xendit"
  ],
  "author": "Bo",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "sqlite3": "^4.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "husky": "^4.2.5",
    "mocha": "^6.1.4",
    "nyc": "^15.1.0",
    "supertest": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "pre-push": "npm run test"
    }
  }
}
